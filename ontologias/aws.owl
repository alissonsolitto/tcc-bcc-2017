    <!-- http://purl.oclc.org/NET/ssnx/meteo/aws#Albedometer -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/meteo/aws#Albedometer">
        <rdfs:subClassOf rdf:resource="http://purl.oclc.org/NET/ssnx/meteo/aws#RadiationSensor"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.oclc.org/NET/ssnx/ssn#observes"/>
                <owl:someValuesFrom rdf:resource="http://purl.oclc.org/NET/ssnx/qu/dim#Dimensionless"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source>skos:closeMatch albedometer [GAMP 2.3.1.1] http://www.wmo.int/pages/prog/wcp/agm/gamp/gamp_en.html</dc:source>
        <rdfs:comment>pair of upward and downward facing solarimeters</rdfs:comment>
        <rdfs:label>albedometer</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.oclc.org/NET/ssnx/meteo/aws#AneroidDisplacementTransducer -->

    <owl:Class rdf:about="http://purl.oclc.org/NET/ssnx/meteo/aws#AneroidDisplacementTransducer">
